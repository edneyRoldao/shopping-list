  "bugs": {
    "url": "https://github.com/edneyRoldao/shopping-list/issues"
  },
  "homepage": "https://github.com/edneyRoldao/shopping-list#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "dotenv": "^8.2.0",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv-expand": "^5.1.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
